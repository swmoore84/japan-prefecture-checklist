  <div class="card-visit">
    <h2> <%= visit.place %></h2>
    <br>
    <p> <%= visit.description %> </p>
    <p> <%= visit.date_from %> </p>
    <p><%= visit.date_to %> </p>
    <p><%= link_to visit_path(visit), method: :delete, data: { confirm: "Delete this visit?" } do %>
      <i class='far fa-trash-alt'></i>
    <% end %>
      <%= link_to "Edit", edit_visit_path(visit) %></p>
  </div>
